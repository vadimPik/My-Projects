<Window x:Class="Wpf_mvvm.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wpf_mvvm"
        xmlns:views = "clr-namespace:Wpf_mvvm.Views"
        mc:Ignorable="d"
        DataContext="{Binding Path=MainTabsViewModel, Source={StaticResource ServiceLocator}}"
        Title="My WPF Project" Height="450" Width="800">
    <Grid>
        <views:MainTabsView x:Name = "MainTabsViewControl"/>
    </Grid>
</Window>
